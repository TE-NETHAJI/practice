    <?xml version="1.0" encoding="UTF-8"?>
    <beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	
	<bean id="empProps" class ="com.te.springcore.beans.Employee">
	<property name="id" value="200"></property>
	<property name="name" value ="Nethaji"></property>
	<property name="role" value ="Developer"></property>
	<property name="dept" ref="dept"></property>
	</bean>
	
	<bean id="dept" name="com.te.springcore.beans.DeptClass">
	<property name="deptId" value="100"/>
	<property name="deptName" value="Testing"/>
	</bean>
	
	
	
	

</beans>